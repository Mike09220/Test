void drawCloud(int CloudX, int CloudY){
  
  //Draws circles for colud...
    
  noStroke();
  fill(230,230,230);
  rect(CloudX,CloudY,70,30,50);
  
  noStroke();
  fill(230,230,230);
  ellipse(CloudX+15,CloudY+10,25,25);
  
  noStroke();
  fill(230,230,230);
  ellipse(CloudX+25,CloudY+10,35,35);
  
    noStroke();
  fill(230,230,230);
  ellipse(CloudX+35,CloudY+10,40,40);
  
    noStroke();
  fill(230,230,230);
  ellipse(CloudX+45,CloudY+10,35,35);
  
    noStroke();
  fill(230,230,230);
  ellipse(CloudX+55,CloudY+10,25,25);
  
  
  
}
